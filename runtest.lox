// declarations and scoping
var a = "global a";
var b = "global b";
var c = "global c";
{
  var a = "outer a";
  var b = "outer b";
  {
    var a = "inner a";
    print a;
    print b;
    print c;
  }
  print a;
  print b;
  print c;
}
print a;
print b;
print c;


// if-else
if (true or false){
  print "outer if";
  
  if (false and true) print "inner if";
  else print "inner else";
}
else print "outer else";


// loops
var i = 0;
while (i < 5) {
  print i;
  i = i + 1;
}
